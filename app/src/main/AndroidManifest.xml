<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thethrillathand">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sample"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".WeatherActivity"/>
        <activity android:name=".PoinActivity" />
        <activity android:name=".fishsearch.SpotActivity"/>
        <activity android:name=".knot.uninote" android:theme="@style/Theme.mDialog"/>
        <activity android:name=".knot.clinch" android:theme="@style/Theme.mDialog"/>
        <activity android:name=".knot.outside" android:theme = "@style/Theme.mDialog"/>
        <activity android:name=".knot.yarn" android:theme = "@style/Theme.mDialog"/>
        <activity android:name=".preparations.bass" android:theme = "@style/Theme.mDialog"/>
        <activity android:name=".preparations.hairtail" android:theme = "@style/Theme.mDialog"/>
        <activity android:name=".preparations.rockfish" android:theme = "@style/Theme.mDialog"/>
        <activity android:name=".preparations.sanddab" android:theme = "@style/Theme.mDialog"/>
        <activity
            android:name=".Login"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".noticeActivity" />
        <activity android:name=".configurationActivity" />
        <activity android:name=".eventActivity" />
        <activity android:name=".SOSActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".fishsearch.SearchActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>