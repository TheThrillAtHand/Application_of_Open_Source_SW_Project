<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:layout_width="143dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/donghae" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="동해 낚시터"
                android:paddingLeft="10dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"


                />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="장소: 강원도 동해시"
                android:paddingLeft="10dp"
                />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="번호: 000-0000-0000"
                android:paddingLeft="10dp"
                />

            <Button
                android:id="@+id/product_fishery1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/fisherycolor"
                android:fontFamily="@font/malgun"
                android:text="상세보기"
                android:textColor="@color/white"
                android:textStyle="bold" />

        </LinearLayout>


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:layout_width="143dp"
            android:layout_height="match_parent">

            <ImageView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/dangjin" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="당진 낚시터"
                android:paddingLeft="10dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"

                />

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="장소: 충청남도 당진시"
                android:paddingLeft="10dp"
                />

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="번호: 000-0000-0000"
                android:paddingLeft="10dp"
                />

            <Button
                android:id="@+id/product_fishery2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background = "@color/fisherycolor"
                android:textColor="@color/white"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"
                android:text="상세보기" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:layout_width="143dp"
            android:layout_height="match_parent">

            <ImageView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/gumi" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="구미 낚시터"
                android:paddingLeft="10dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"

                />

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="장소: 경상북도 구미시"
                android:paddingLeft="10dp"
                />

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="번호: 000-0000-0000"
                android:paddingLeft="10dp"
                />

            <Button
                android:id="@+id/product_fishery3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background = "@color/fisherycolor"
                android:textColor="@color/white"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"
                android:text="상세보기" />

        </LinearLayout>



    </LinearLayout>
</LinearLayout>