                android:layout_height="wrap_content"
                android:text="동해 낚시터"
                android:paddingLeft="10dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:fontFamily="@font/malgun"
                android:textStyle="bold"


                />

            <TextView
